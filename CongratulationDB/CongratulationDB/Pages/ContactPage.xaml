<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="CongratulationDB.Pages.ContactPage">
    <ContentPage.Content>
        <StackLayout>
            <Entry x:Name="nameEntry" Placeholder="Enter name"/>
            <Entry x:Name="surnameEntry" Placeholder="Enter surname"/>
            <Entry x:Name="phoneEntry" Placeholder="Enter phone"/>
            <Entry x:Name="emailEntry" Placeholder="Enter email"/>
            <Button Text="Add" Clicked="Button_Clicked"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>